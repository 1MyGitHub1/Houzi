            <ColumnDefinition Width="578"/>
            <ColumnDefinition Width="56"/>
            <ColumnDefinition Width="484"/>
            <ColumnDefinition Width="6"/>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="24"/>
            <RowDefinition/>
