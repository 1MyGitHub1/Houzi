                                <Image Source="/Totalab_L;component/Images/zup@2x.png" Style="{StaticResource Image48Sty}"/>
                            </Viewbox>
                            <TextBlock Text="Up1mm" Style="{StaticResource TextBlockSty}"  FontSize="10"  VerticalAlignment="Bottom"/>
                        </StackPanel>
                    </Button.Content>
                </Button>
                <Button Style="{StaticResource  ButtonPrimarySty}"  Width="74" Grid.Row="6" Grid.Column="2" HorizontalAlignment="Right">
