                            </Viewbox>
                            <TextBlock Text="Dn1mm" Style="{StaticResource TextBlockSty}"  FontSize="10"  VerticalAlignment="Bottom"/>
                        </StackPanel>
                    </Button.Content>
                </Button>
                <Button Style="{StaticResource  ButtonPrimarySty}"  Width="74" Grid.Row="10" Grid.Column="2" HorizontalAlignment="Right">
