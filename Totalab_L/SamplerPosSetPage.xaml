                    <RadioButton Style="{StaticResource RadioButtonTextSty}" Content="Front left" Width="96" Margin="24 0 0 0"/>
                </StackPanel>
                <Button Style="{StaticResource ButtonIconPrimarySty}" Width="56" Grid.Row="4">
