                        </Button.Content>
                    </Button>
                    <Button Style="{StaticResource ButtonIconPrimarySty}" Width="56" Margin="4 0 0 0">
                        <Button.Content>
                            <Image Source="/Totalab_L;component/Images/right@2x.png" Style="{StaticResource Image48Sty}"/>
                        </Button.Content>
                    </Button>
                </StackPanel>
                <Button Style="{StaticResource ButtonIconPrimarySty}" Width="56" Grid.Row="8">
                    <Button.Content>
                        <Image Source="/Totalab_L;component/Images/down_s@2x.png" Style="{StaticResource Image48Sty}"/>
                    </Button.Content>
                </Button>
                <StackPanel Grid.Row="10" Orientation="Horizontal">
                    <RadioButton Style="{StaticResource RadioButtonTextSty}" Content="Rear right" Width="96"></RadioButton>
                    <Button Style="{StaticResource ButtonIconPrimarySty}" Width="56" Margin="24 0 0 0">
                        <Button.Content>
                            <Image Source="/Totalab_L;component/Images/down@2x.png" Style="{StaticResource Image48Sty}"/>
                        </Button.Content>
                    </Button>
                    <RadioButton Style="{StaticResource RadioButtonTextSty}" Content="Front right" Width="96" Margin="24 0 0 0"/>
                </StackPanel>
                <TextBlock Text="进样针"  Style="{StaticResource TextBlockSty}" Grid.Column="2"/>
