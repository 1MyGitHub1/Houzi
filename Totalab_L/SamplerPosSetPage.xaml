                <TextBlock Style="{StaticResource TextBlockSty}"  Text="Tray A"  FontSize="14"  FontFamily="SegoeUI" FontWeight="SemiBold"  VerticalAlignment="Top" Margin="0 5"/>
                <Border BorderBrush="Black" BorderThickness="1" Width="504" Height="120" Grid.Column="2">
