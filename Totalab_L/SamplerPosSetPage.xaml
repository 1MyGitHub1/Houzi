                                                ItemsSize="{Binding TrayDInfos.ItemsSize, Source={x:Static global:GlobalInfo.Instance}}">
                        <themes:TrayControl.ItemTemplate>
                            <DataTemplate>
                                <Grid>
                                    <Ellipse Fill="#3C000000"/>
                                    <TextBlock Text="{Binding ItemContent}" Foreground="White" HorizontalAlignment="Center" FontSize="12"/>
                                </Grid>
                            </DataTemplate>
                        </themes:TrayControl.ItemTemplate>
                    </themes:TrayControl>
                </Border>
            </Grid>
            <Grid>
