                            </Viewbox>
                            <TextBlock Text="Dn10mm" Style="{StaticResource TextBlockSty}"  FontSize="10"  VerticalAlignment="Bottom"/>
                        </StackPanel>
                    </Button.Content>
                </Button>
            </Grid>
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="位置校准" Style="{StaticResource UCtlTitleTextBlockSty}" FontSize="16"/>
                <!--复位-->
                <Button Style="{StaticResource  ButtonAccentSty}"  Width="74" Content="{DynamicResource AutoSampler_Home}"  Margin="20 0 0 0"
                            Click="HomeCommand">
                    <Button.IsEnabled>
                        <MultiBinding Converter="{StaticResource MultiBoolToObjectConverter}">
                            <MultiBinding.ConverterParameter>
                                <x:Array Type="{x:Type System:Boolean}">
                                    <System:Boolean>True</System:Boolean>
                                    <System:Boolean>True</System:Boolean>
                                </x:Array>
                            </MultiBinding.ConverterParameter>
                            <Binding Path="IsCanRunning" Mode="OneWay" Source="{x:Static global:GlobalInfo.Instance}"/>
                            <Binding Path="IsConnect" Mode="OneWay"/>
                        </MultiBinding>
                    </Button.IsEnabled>
                </Button>
                <!--清错-->
                <Button Style="{StaticResource  ButtonAccentSty}"  Width="74" Content="清错"  Margin="20 0 0 0"
