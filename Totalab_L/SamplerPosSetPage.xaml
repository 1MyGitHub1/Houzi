                                        <System:Boolean>True</System:Boolean>
                                    </x:Array>
                                </MultiBinding.ConverterParameter>
                                <Binding Path="IsCanRunning" Mode="OneWay" Source="{x:Static global:GlobalInfo.Instance}"/>
                                <Binding Path="IsConnect" Mode="OneWay"/>
