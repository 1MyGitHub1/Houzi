                <TextBlock Text="机械臂"  Style="{StaticResource TextBlockSty}"/>
                <StackPanel Grid.Row="2" Orientation="Horizontal">
                    <RadioButton Style="{StaticResource RadioButtonTextSty}" Content="Rear left" Width="96"/>
                    <Button Style="{StaticResource ButtonIconPrimarySty}" Width="56" Margin="24 0 0 0">
